---
title: "Homeowner Control Period (2026-Present)"
---
graph TB
  classDef person fill:#4A90D9,color:#fff,stroke:#2C5F8A,stroke-width:2px
  classDef entity fill:#F5A623,color:#fff,stroke:#C47D0E,stroke-width:2px
  classDef hoa fill:#7B68EE,color:#fff,stroke:#5A4CB5,stroke-width:3px
  classDef vendor fill:#50C878,color:#fff,stroke:#2E8B57,stroke-width:2px
  classDef mgmt fill:#E74C3C,color:#fff,stroke:#C0392B,stroke-width:2px
  classDef homeowners fill:#4A90D9,color:#fff,stroke:#2C5F8A,stroke-width:2px,stroke-dasharray: 5 5

  Owners["7 Homeowner<br/>Board Members"]:::homeowners
  Board_H["Board"]:::entity
  HOA_H["Deschutes Heights HOA<br/>(WA non-profit)"]:::hoa

  subgraph LANDOWNERS ["Landowners (no governance)"]
    direction LR
  Min_H["Min<br/>(not on Board)"]:::person
    SOUK_H["SO UK Investments"]:::entity
    Lotus_H["Lotus House"]:::entity
  end

  subgraph VENDORS ["Contracted Services"]
    direction LR
  VIS_H["VIS Group<br/>(Management Co.)"]:::mgmt
  SHR_H["Simply Home Realty<br/>(Vendor)"]:::vendor
  PG_H["Precision Groundworks<br/>(Vendor)"]:::vendor
  end

  Owners -- "control" --> Board_H
  Board_H -- "governs" --> HOA_H

  Min_H -- "controls" --> SOUK_H
  Min_H -- "controls" --> Lotus_H
  SOUK_H -. "landowner" .-> HOA_H
  Lotus_H -. "landowner / builder" .-> HOA_H

  HOA_H -- "contracts" --> VIS_H
  HOA_H -- "contracts" --> SHR_H
  HOA_H -- "contracts" --> PG_H

  VIS_H -. "selects" .-> SHR_H
  VIS_H -. "selects" .-> PG_H
